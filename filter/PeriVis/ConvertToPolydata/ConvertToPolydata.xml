<ServerManagerConfiguration>
  <ProxyGroup name="filters">
    <SourceProxy name="ConvertToPolydata" class="vtkConvertToPolydata" label="ConvertToPolydata">
    <Documentation
        long_help="Calculate and expose ConvertToPolydata"
        short_help="ConvertToPolydata">
    </Documentation>
    <InputProperty
        name="Input"
        command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
      <DataTypeDomain name="input_type">
        <DataType value="vtkDataSet" />
      </DataTypeDomain>
    </InputProperty>
    
    </SourceProxy>

  </ProxyGroup>
  <!-- End Filters Group -->
</ServerManagerConfiguration>
