<ServerManagerConfiguration>
  <ProxyGroup name="filters">
    <SourceProxy name="AddTime" class="vtkAddTime" label="AddTime">
    <Documentation
        long_help="Calculate and expose AddTime"
        short_help="AddTime">
    </Documentation>
    <InputProperty
        name="Input"
        command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
      <DataTypeDomain name="input_type">
        <DataType value="vtkDataSet" />
      </DataTypeDomain>
      <Documentation>
        This property specifies the input to the AddTime filter.
      </Documentation>
    </InputProperty>
  
    </SourceProxy>

  </ProxyGroup>
  <!-- End Filters Group -->
</ServerManagerConfiguration>
